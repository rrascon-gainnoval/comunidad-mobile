{
  "expo": {
    "name": "Comunidad",
    "slug": "comalta",
    "version": "1.4.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.4.5",
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Permite acceder a la galería de fotos para subir imágenes"
      },
      "bundleIdentifier": "com.innoval.comalta"
    },
    "android": {
      "versionCode": 10,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "package": "com.innoval.comalta",
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/images/icon.png"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e016f192-5c02-4ba5-80c5-4dd48f9d8cbe"
      }
    },
    "owner": "innoval",
    "plugins": [
      "expo-font"
    ]
  }
}
